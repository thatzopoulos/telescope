{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "types": ["./worker-configuration.d.ts", "node"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@/generated/*": ["./generated/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
